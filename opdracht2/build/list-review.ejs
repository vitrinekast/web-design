


<% data.forEach(function(item) { %>
  
  <%
  let firstId = 'false';
  
    if(item.files) {
      firstId = item.files.find((file) => {
        
        return !file.reviewed
      }).id
      
    }
   %>
   
   
<li class=''>
  <a href="/review/<%= item.id %>/<%= firstId %>" data-navigo class='list__item'>
    <div class="">
      <h3>
        <%= item.repoName %>
      </h3>
      <h4>
        <%= item.student %>
      </h4>
    </div>
    <div class="">
      <i class="material-icons icon icon--lg"> navigate_next </i>
    </div>
  </a>
</li>
<% }) %>